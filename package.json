{
  "name": "@donews/nestjs-logger",
  "version": "0.4.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint \"{lib,test}/**/*.ts\" --fix",
    "format": "prettier --write \"lib/**/*.ts\" \"test/**/*.ts\"",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test": "jest --config ./test/jest-e2e.json --runInBand",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "dependencies": {
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-angular": "8.3.4",
    "@nestjs/common": "7.0.13",
    "@nestjs/core": "7.0.13",
    "@nestjs/platform-express": "7.0.13",
    "@nestjs/testing": "7.0.13",
    "@types/jest": "25.2.2",
    "@types/node": "12.12.34",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.4",
    "prettier": "2.0.5",
    "reflect-metadata": "0.1.13",
    "release-it": "13.6.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.5.1",
    "typescript": "3.9.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && lint-staged && npm run test",
      "pre-push": "npm run build"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DoNewsCode/nestjs-logger"
  }
}
